  "jest": {
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "react-app-env.d.ts",
      "./assets",
      "./styles",
      "./hooks/use-fetch",
      "stories.tsx",
      ".hooks/apis/mockApis.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
