  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BUILD_PATH=build/META-INF/resources react-scripts build",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter",
    "sonarqube": "npm run coverage && sonar-scanner",
    "lint": "eslint src/ && stylelint \"src/**/*.scss\"",
    "format": "eslint src/ --fix && stylelint \"src/**/*.scss\" --fix",
