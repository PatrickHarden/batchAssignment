{
  "name": "@scholastic/ba-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check-env": "node -e 'console.log(process.env)'",
    "pre-commit": "run-s lint format",
    "bdd": "cross-env run-s --continue-on-error bdd:*",
    "bdd:setup": "node bdd-setup.js",
    "bdd:cucumber": "cucumber-js --parallel $THREADS --tags \"$TAGS\" features/**/*.feature --require './src/**/*.ts' --require-module ts-node/register --require-module dotenv/config --format-options '{\"snippetInterface\": \"async-await\"}' --format html:reports/cucumber-report.html --format json:reports/cucumber-report.json --format summary --publish-quiet --exit",
    "lint": "eslint --fix --ext ts,tsx ./src",
    "format": "pretty-quick --staged"
  },
  "dependencies": {
    "@cucumber/cucumber": "7.3.1",
    "@scholastic/volume-test": "1.27.2",
    "@types/chai": "^4.3.3",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "dotenv": "10.0.0",
    "moment": "^2.29.4",
    "request": "^2.88.2",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "7.3.1",
    "@rushstack/eslint-patch": "1.0.6",
    "@types/chromedriver": "^81.0.1",
    "@types/jquery": "^3.5.14",
    "@types/node": "12.20.1",
    "@types/selenium-webdriver": "4.0.16",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/experimental-utils": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "@zerollup/ts-transform-paths": "1.7.9",
    "chromedriver": "^109.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "~17.0.1",
    "selenium-webdriver": "4.0.0",
    "sonarqube-scanner": "^3.0.0",
    "ts-node": "^10.4.0"
  }
}
